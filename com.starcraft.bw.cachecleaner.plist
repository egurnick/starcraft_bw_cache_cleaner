<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.starcraft.bw.cachecleaner</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/starcraft_bw_cache_cleaner.sh</string>
    </array>

    <key>StartInterval</key>
    <integer>60</integer>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/starcraft_bw_cachecleaner.out</string>

    <key>StandardErrorPath</key>
    <string>/tmp/starcraft_bw_cachecleaner.err</string>
</dict>
</plist>

